class Main
{
    public static void main(String args[])
    {
        String inputStr ="srtteerress";
        boolean letter = true;
        System.out.println("Typed word: "+ inputStr);
        for(char i :inputStr.toCharArray())
        {
            if (inputStr.indexOf(i) == inputStr.lastIndexOf(i))
            {
                System.out.println("First non repeating letter is: "+ i);
                letter = false;
                break;
            }
        }

        if(letter)
            System.out.println("NONE");
    }
}
